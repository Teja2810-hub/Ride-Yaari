# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Places API (for location autocomplete)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key

# Error Notification Configuration (for Supabase Edge Functions)
# Add these to your Supabase project settings > Edge Functions > Environment Variables

# Discord Webhook (optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_url

# Slack Webhook (optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your_webhook_url

# Email Notifications via Resend API (optional)
EMAIL_API_KEY=your_resend_api_key
DEVELOPER_EMAIL=your_email@domain.com

# Alternative: SendGrid API (optional)
SENDGRID_API_KEY=your_sendgrid_api_key

# Alternative: Mailgun API (optional)
MAILGUN_API_KEY=your_mailgun_api_key
MAILGUN_DOMAIN=your_mailgun_domain